{
  "name": "be.live",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "viewer/admin",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "dotenv -e .env.development npx sequelize-cli db:migrate --migrations-path src/migrations --config src/config/config.json",
    "migrate:production": "dotenv -e .env.production npx sequelize-cli db:migrate --migrations-path src/migrations --config src/config/config.json",
    "seed": "dotenv -e .env.development npx sequelize-cli db:seed:all --seeders-path src/seeders --config src/config/config.json",
    "dev": "concurrently \"D:/Redis/redis-server.exe\" \"nodemon\"",
    "seed:production": "dotenv -e .env.production npx sequelize-cli db:seed:all --seeders-path src/seeders --config src/config/config.json",
    "start": "concurrently \"D:/Redis/redis-server.exe\" \"dotenv -e .env.development nodemon src/API/index.ts\"",
    "production": "dotenv -e .env.production node src/server.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public",
      "src/media/*"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register src/API/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "be.live": "file:",
    "body-parser": "^1.20.3",
    "concurrently": "^9.1.2",
    "cookie-parser": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "fs": "^0.0.1-security",
    "helmet": "^8.1.0",
    "jet-logger": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.4",
    "node-cache": "^5.1.2",
    "node-media-server": "^2.6.0",
    "nodemailer": "^6.9.16",
    "nodemailer-plugin-inline-base64": "^2.1.1",
    "path": "^0.12.7",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "ts-command-line-args": "^2.5.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node-media-server": "^2.3.7",
    "@types/nodemailer": "^6.4.17",
    "@types/ws": "^8.5.12",
    "dotenv-cli": "^7.4.2",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
